{
    "compilerOptions": {
        "module": "None",
        "target": "ES6",
        "allowUnreachableCode": false,
        "allowUnusedLabels": false,
        "allowJs": false,
        "alwaysStrict": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "strict": true,
        "strictFunctionTypes": true,
        "strictNullChecks": true,
        "outFile": "js/main.js",
        "lib": ["es2018", "dom"]
    },
    "files": [
        "src/main.ts"
    ]
}
